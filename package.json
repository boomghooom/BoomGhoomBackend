    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "crypto-js": "^4.2.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "bull": "^4.12.0",
    "bullmq": "^5.1.0",
    "aws-sdk": "^2.1517.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
