{
  "name": "boomghoom-backend",
  "version": "1.0.0",
  "description": "Backend API for BoomGhoom - Social Events & Group Activities Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:migrate": "tsx src/scripts/migrate.ts"
  },
  "keywords": [
    "events",
    "social",
    "group-activities",
    "india",
    "nodejs",
    "typescript"
  ],
  "author": "BoomGhoom Team",
  "license": "UNLICENSED",
